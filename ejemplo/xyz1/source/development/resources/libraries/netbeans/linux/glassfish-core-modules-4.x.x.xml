<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.netbeans.org/ns/library-declaration/3" version="3.0">
    <name>glassfish-core-modules</name>
    <type>j2se</type>
    <description>GlassFish modules used by the core library</description>
    <volume>
        <type>classpath</type>
        <resource>jar:file:/opt/glassfish-4.0/glassfish/modules/javax.el.jar!/</resource>
        <resource>jar:file:/opt/glassfish-4.0/glassfish/modules/javax.faces.jar!/</resource>
        <resource>jar:file:/opt/glassfish-4.0/glassfish/modules/javax.jms-api.jar!/</resource>
        <resource>jar:file:/opt/glassfish-4.0/glassfish/modules/javax.servlet-api.jar!/</resource>
        <resource>jar:file:/opt/glassfish-4.0/glassfish/modules/javax.servlet.jsp.jar!/</resource>
    </volume>
    <volume>
        <type>src</type>
    </volume>
    <volume>
        <type>javadoc</type>
    </volume>
    <volume>
        <type>maven-pom</type>
    </volume>
</library>
